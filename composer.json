	"repositories": [{
			"type": "vcs",
			"url": "https://github.com/BookOfOrigin/Ori"
	}],
	"require": {
		"smarty/smarty": "~5.3",
		"mailgun/mailgun-php": "~4.2",
		"paragonie/random_compat": "^2.0",
		"php-http/guzzle7-adapter": "^1.0",
		"phpseclib/phpseclib" : "^3.0.39",
		"bookoforigin/slashtrace": "dev-master"
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true
